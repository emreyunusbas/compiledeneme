{
  "name": "pilates-studio-management",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "start:go": "EXPO_GO_DEBUG=true expo start --dev-client",
    "start:lan": "expo start --lan",
    "start:tunnel": "expo start --tunnel",
    "start:mobile": "expo start --lan --clear",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "expo build:web",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "build:apk": "expo build:android --type apk",
    "build:bundle": "expo build:android --type app-bundle",
    "ts:check": "tsc --noEmit",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "clean": "expo start --clear",
    "reset": "expo install --fix",
    "install:clean": "rm -rf node_modules package-lock.json && npm install",
    "doctor": "expo doctor",
    "install:deps": "npx expo install",
    "update:deps": "npx expo install --fix",
    "start:backend": "cd backend && npm run dev",
    "docker:backend": "make dev-db",
    "precommit": "npm run lint && npm run typecheck",
    "test": "echo 'Tests coming soon!'",
    "prepare": "echo 'Project setup complete'",
    "eject": "echo 'Do not eject this project'"
  },
  "dependencies": {
    "react": "19.0.0",
    "react-native": "0.79.1",
    "expo": "~53.0.4",
    "expo-router": "~5.0.3",
    "typescript": "~5.8.3",
    "@nkzw/create-context-hook": "~1.1.0",
    "@tanstack/react-query": "~5.90.5",
    "@react-native-async-storage/async-storage": "~2.1.2",
    "zustand": "~5.0.2",
    "lucide-react-native": "~0.475.0",
    "expo-linear-gradient": "~14.1.4",
    "react-native-safe-area-context": "~5.3.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-svg": "~15.11.2",
    "trpc": "~11.6.0",
    "hono": "~4.10.1",
    "zod": "~4.1.12",
    "superjson": "~2.2.2",
    "expo-image": "~2.1.6",
    "expo-haptics": "~14.1.4",
    "expo-image-picker": "~16.1.4",
    "expo-location": "~18.1.4",
    "expo-status-bar": "~2.1.1",
    "react-native-screens": "~4.5.0",
    "react-native-reanimated": "~3.16.1",
    "expo-font": "~12.0.4",
    "expo-constants": "~17.0.2",
    "expo-notifications": "~0.29.8"
  },
  "devDependencies": {
    "@types/react": "~19.0.1",
    "@types/react-native": "~0.73.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-expo": "^8.0.0",
    "prettier": "^3.0.0"
  },
  "keywords": [
    "pilates",
    "studio",
    "management",
    "react-native",
    "expo",
    "typescript"
  ],
  "author": "Pilates Studio Management Team",
  "license": "MIT",
  "description": "A comprehensive Pilates Studio Management mobile application for studio owners, trainers, and members."
}