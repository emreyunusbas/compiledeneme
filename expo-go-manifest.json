{
  "expo": {
    "name": "Pilates Studio",
    "slug": "pilates-studio-go",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1A1D23"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pilates.studio.go",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#1A1D23"
      },
      "package": "com.pilates.studio.go",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-image-picker",
      "expo-location",
      "expo-notifications",
      "expo-haptics",
      "expo-linear-gradient",
      "expo-image"
    ],
    "scheme": "pilates-studio",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "apiUrl": "http://localhost:3000"
    },
    "runtimeVersion": {
      "policy": "nativeVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/update/your-project-id",
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "owner": "your-username"
  }
}